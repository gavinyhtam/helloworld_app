	<script>
	//Source for FB SDK Loaded from http://stackoverflow.com/questions/7747668/fb-is-undefined-even-though-i-just-used-it
		(function($) {
			$(document).bind("FBSDKLoaded", function() {
				FB.getLoginStatus(function(response) {
					if (response.status === "connected") {
						getPhotos();
					}
				});
			});
		})(jQuery);
		
		function getPhotos() {
			FB.api("/me/photos/uploaded",
				function(response) {
					console.log(response.data[0]);
					//<!-- BEGIN TimelineJS -->
						$(document).ready(function() {
							var photos = { 
								timeline: {
									headline: "Singapore trip",
									type: "default",
									text: "<p>Album</p>",
									asset: {
										media: response.data[0].source
									},
									date: [
										{
											startDate: parseDate(response.data[0].created_time),
											endDate: parseDate(response.data[0].created_time),
											headline: response.data[0].name,
											text: "<p>Singapore photo</p>",
											asset: {
												media: response.data[0].source
											}
										}
									],
									era: [
										{
											startDate: parseDate(response.data[0].created_time),
											endDate: parseDate(response.data[0].created_time),
											headline: response.data[0].name,
											text: "<p>Singapore photo</p>",
											asset: {
												media: response.data[0].source
											}
										}
									],
								}
							};
							 createStoryJS({
								  type:       'timeline',
								  width:      '800',
								  height:     '600',
								  source:     photos,
								  embed_id:   'my-timeline'
							 });
							 //Will add loop to go to length and place new photo
							 console.log(photos["timeline"]["date"][0]["startDate"]);
							 //var date = response.data[0].created_time.replace(/-/g, ",").substring(0,10);
							 //console.log(date);		 
						});
				  //<!-- END TimelineJS -->
					//document.getElementById("myFBImage").innerHTML = "<img src=" + response.data[0].source + ">";
			});
		}
		
		//parses JSON (ISO8601) to TimelineJS Date format
		function parseDate(string) {
			//return string.replace(/-/g, ",").substring(0,10);
			var date = new Date(string);
			//date.getMonth() returns 0-11, instead of 1-12
			return date.getFullYear() + "," + (date.getMonth()+1) + "," + date.getUTCDate();
		}
